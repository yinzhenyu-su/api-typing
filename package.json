{
  "name": "api-typing",
  "version": "0.1.1",
  "description": "Axios based HTTP client with type hint",
  "keywords": [
    "api",
    "typescript",
    "axios",
    "http",
    "type",
    "hint"
  ],
  "author": {
    "email": "sharkrain@outlook.com",
    "name": "yinzhenyu",
    "url": "https://blog.bgm.fun"
  },
  "homepage": "https://github.com/yinzhenyu-su/api-typing",
  "maintainers": [
    "yinzhenyu-su"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "outdir",
    "bin",
    "src"
  ],
  "bin": {
    "get-types": "./bin/cli.mjs"
  },
  "main": "./outdir/index.js",
  "module": "./outdir/index.mjs",
  "types": "./src/index.ts",
  "scripts": {
    "build-fast": "tsup",
    "build": "pnpm run build-fast -- --dts-resolve",
    "test": "vitest run && tsc --project ./tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.185",
    "@types/node": "14",
    "@types/qs": "^6.9.7",
    "@vue/reactivity": "^3.2.45",
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "openapi-typescript": "^6.2.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@types/jsdom-global": "^3.0.4",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "prettier": "2.8.3",
    "tsup": "^6.5.0",
    "typescript": "4.9.4",
    "vitepress": "1.0.0-alpha.49",
    "vitest": "0.27.1",
    "vue": "^3.2.45"
  }
}